msgstr "Nombres de funciones del int@'erprete."

#. type: item
#: rluser.texi:2076
#, no-wrap
msgid "group"
msgstr "group"

#. type: table
#: rluser.texi:2078
msgid "Group names.  May also be specified as @option{-g}."
msgstr "Nombres de grupo.  Tambi@'en puede ser especificado como @option{-g}."

#. type: item
#: rluser.texi:2079
#, no-wrap
msgid "helptopic"
msgstr "helptopic"

#. type: table
#: rluser.texi:2081
