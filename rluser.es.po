msgstr "backward-char (C-b)"

#. type: Plain text
#: rluser.texi:1122
msgid "Move back a character."
msgstr "Se mueve atr@'as un car@'acter."

#. type: item
#: rluser.texi:1123
#, no-wrap
msgid "forward-word (M-f)"
msgstr "forward-word (M-f)"

#. type: Plain text
#: rluser.texi:1126
