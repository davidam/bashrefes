#: rluser.texi:2038
#, no-wrap
msgid "-A @var{action}"
msgstr "-A @var{acci@'on}"

#. type: table
#: rluser.texi:2041
msgid "The @var{action} may be one of the following to generate a list of possible completions:"
msgstr "La @var{acci@'on} puede ser una de las siguientes para generar una lista de posibles compleciones:"

