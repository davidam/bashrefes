#~ "    *)      cur=$2 ;;\n"
#~ "    esac\n"
#~ "\n"
#~ msgstr ""
#~ "    # Expansión de virgulilla, con el efecto secundario\n"
#~ "    # de expandir la virgulilla al nombre de ruta completo\n"
#~ "    case \"$2\" in\n"
#~ "    \\~*)    eval cur=\"$2\" ;;\n"
#~ "    *)      cur=$2 ;;\n"
#~ "    esac\n"
#~ "\n"
